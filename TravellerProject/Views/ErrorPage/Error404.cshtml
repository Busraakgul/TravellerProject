@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Error 404 - Page Not Found!</title>
    <link rel="stylesheet" href="~/simple-404-template-master/assets/css/style.css">
    <link href="~/simple-404-template-master/assets/images/favicon.png" rel="icon" type="image/x-icon">
</head>
<body>
    <div class="container">
        <img class="ops" src="~/simple-404-template-master/assets/images/404.jpg" alt="404 Error - Page Not Found">
        <h3>Oops! The page you are looking for cannot be found.</h3>
        <p>It might have been removed, had its name changed, or is temporarily unavailable.</p>
        <a class="button" href="/Default/Index">Return to Home</a>
    </div>
</body>
</html>
